{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-cli-plugin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/ng-cli-plugin",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "scripts": [],
            "polyfills": "src/polyfills.ts",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "center-dll": {
              "outputPath": "dist/center-dll",
              "vendorChunk": false,
              "assets": [],
              "styles": [],
              "customWebpackConfig": {
                "path": "./webpack.config.center-dll.ts"
              }
            },
            "center-dll-prod": {
              "outputPath": "dist/center-dll",
              "vendorChunk": false,
              "assets": [],
              "styles": [],
              "customWebpackConfig": {
                "path": "./webpack.config.center-dll.ts"
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true
            },
            "center-main": {
              "vendorChunk": false,
              "index": {
                "input": "src/index.center-dll.html",
                "output": "index.html"
              },
              "assets": [
                {
                  "glob": "dll.js",
                  "input": "./dist/center-dll",
                  "output": "./"
                }
              ],
              "customWebpackConfig": {
                "path": "./webpack.config.center-main.ts"
              }
            },
            "center-main-prod": {
              "baseHref": "/ng-cli-plugin-demo/",
              "vendorChunk": false,
              "index": {
                "input": "src/index.center-dll.html",
                "output": "index.html"
              },
              "assets": [
                {
                  "glob": "dll.js",
                  "input": "./dist/center-dll",
                  "output": "./"
                }
              ],
              "customWebpackConfig": {
                "path": "./webpack.config.center-main.ts"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "ng-cli-plugin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-cli-plugin:build:production"
            },
            "serve": {
              "browserTarget": "ng-cli-plugin:build:serve"
            },
            "center-main": {
              "browserTarget": "ng-cli-plugin:build:center-main"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-cli-plugin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-cli-plugin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-cli-plugin:serve:production"
            }
          }
        }
      }
    },
    "sub1": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/sub1",
      "sourceRoot": "projects/sub1/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/sub1",
            "index": "",
            "main": "projects/sub1/src/main.ts",
            "polyfills": "projects/sub1/src/polyfills.ts",
            "tsConfig": "projects/sub1/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/sub1/src/favicon.ico",
              "projects/sub1/src/assets"
            ],
            "styles": ["projects/sub1/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "./webpack.config.sub1.ts"
            },
            "vendorChunk": false,
            "deployUrl": "http://127.0.0.1:4201/"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/sub1/src/environments/environment.ts",
                  "with": "projects/sub1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "deployUrl": "/ng-cli-plugin-demo/sub1/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "sub1:build",
            "publicHost": "0.0.0.0:4201",
            "port": 4201
          },
          "configurations": {
            "production": {
              "browserTarget": "sub1:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sub1:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sub1/src/test.ts",
            "polyfills": "projects/sub1/src/polyfills.ts",
            "tsConfig": "projects/sub1/tsconfig.spec.json",
            "karmaConfig": "projects/sub1/karma.conf.js",
            "assets": [
              "projects/sub1/src/favicon.ico",
              "projects/sub1/src/assets"
            ],
            "styles": ["projects/sub1/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sub1/tsconfig.app.json",
              "projects/sub1/tsconfig.spec.json",
              "projects/sub1/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/sub1/e2e/protractor.conf.js",
            "devServerTarget": "sub1:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sub1:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ng-cli-plugin",
  "cli": {
    "analytics": "b9a634ff-466c-445a-bbca-313d2256c0ea"
  }
}
