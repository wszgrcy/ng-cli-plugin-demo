{
  "name": "ng-cli-plugin-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "npx http-server ./dist/ng-cli-plugin -g -b",
    "start:center-main": "ng serve --configuration center-main --port 4300",
    "build:center-main": "ng build --configuration center-main",
    "build:center-main:prod": "ng build --configuration center-main-prod",
    "build:center-dll": "ng build --configuration center-dll",
    "build:center-dll:prod": "ng build --configuration center-dll-prod",
    "build:sub1": "ng build sub1",
    "build:sub1:prod": "ng build sub1 --prod",
    "start:sub1": "ng serve sub1",
    "deploy": "yarn build:center-dll:prod&&yarn build:center-main:prod&&yarn build:sub1:prod&&cpx \"dist/sub1/**/*\" dist/ng-cli-plugin/sub1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.1.1",
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular/cli": "~11.2.7",
    "@angular/compiler-cli": "~11.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/webpack": "^4.41.22",
    "bluebird": "^3.7.2",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-center": "^0.0.2",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bootstrap-assets-plugin": "1.0.8",
    "webpack-ng-dll-plugin": "2.1.10"
  } 
}
